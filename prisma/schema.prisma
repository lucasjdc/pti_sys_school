generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Aluno {
  id             Int    @id @default(autoincrement())
  name           String
  email          String
  dataNascimento String
  cpf            String
  endereco       String
  telefone       String
}

model Professor {
  id             Int    @id @default(autoincrement())
  name           String
  email          String
  cpf            String
  endereco       String
  telefone       String
  diciplina      String
}

model fornecedor {
  id             Int    @id @default(autoincrement())
  name           String
  email          String
  cnpj            String
  endereco       String
  telefone       String
}
